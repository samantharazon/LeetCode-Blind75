#  complement = target - num
#
#  dictionary = { num : index }
#
#  
#
#

def twoSum(nums:int, target):

    for i in range(len(nums)):
        complement = target - nums[i]

        if complement in dict:
            return [ dict[nums[i]], i]
        else:
            dict[complement] = i

    return None

nums = [11,2,15,7]
target = 9
result = twoSum(nums,target)
print(result)